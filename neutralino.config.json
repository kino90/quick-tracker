{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/quick-tracker-react/dist",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["app.*", "window.*", "os.*", "computer.*", "filesystem.*"],
  "modes": {
    "window": {
      "title": "Quick Tracker",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 300,
      "icon": "/quick-tracker-react/public/icon.png",
      "enableInspector": false,
      "exportAuthInfo": true,
      "exitProcessOnClose": false,
      "borderless": false
    }
  },
  "cli": {
    "binaryName": "quick-tracker",
    "resourcesPath": "/quick-tracker-react/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/quick-tracker-react/public/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0",
    "frontendLibrary": {
      "patchFile": "/quick-tracker-react/index.html",
      "devUrl": "http://localhost:3011"
    }
  }
}
